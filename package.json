{
  "name": "react-formatted-input",
  "version": "0.2.1",
  "description": "A component for formatting input data on the fly",
  "main": "lib/index.js",
  "scripts": {
    "build": "./scripts/build.sh",
    "dev:examples": "webpack-dev-server --config ./examples/webpack.config.js",
    "build:examples": "webpack --config ./examples/webpack.config.js",
    "build:examples:prod": "NODE_ENV=production webpack --config ./examples/webpack.config.js",
    "prepublish": "npm run test && npm run build",
    "lint": "eslint './src/**/*.js'",
    "test": "tape -r babel-register -r ./tests/.setup.js ./tests/*.test.js"
  },
  "author": "everdimension",
  "keywords": [
    "react",
    "react-component",
    "masked input",
    "formatted input",
    "parse"
  ],
  "license": "ISC",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0",
    "prop-types": "^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/everdimension/react-formatted-input.git"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "css-loader": "^0.25.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "google-libphonenumber": "^2.0.4",
    "highlight.js": "^9.8.0",
    "jsdom": "^9.8.3",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "style-loader": "^0.13.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "lodash": "^4.16.6"
  }
}
